   "source": [
    "import pandas as pd\n",
    "\n",
    "def combine_two_tables(person: pd.DataFrame, address: pd.DataFrame) -> pd.DataFrame:\n",
    "    result_df = pd.merge(person, address, on = 'personId', how = 'left')\n",
    "    result_df = result_df[['firstName', 'lastName', 'city', 'state']]\n",
    "\n",
    "    return result_df"
   ]
